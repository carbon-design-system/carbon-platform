    "@mdx-js/loader": "^2.0.0",
    "@next/mdx": "^12.1.0",
    "@octokit/core": "^3.5.1",
    "cache-manager": "^3.6.0",
    "cache-manager-fs-hash": "^1.0.0",
    "clsx": "^1.1.1",
    "cookie-parser": "^1.4.6",
    "express-session": "^1.17.2",
    "fs-extra": "^10.0.0",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "minimatch": "^3.1.2",
    "next": "^12.1.0",

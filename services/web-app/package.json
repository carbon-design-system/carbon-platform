{
  "name": "@carbon-platform/web-app",
  "version": "0.1.98",
  "description": "Carbon web application",
  "homepage": "https://github.com/carbon-design-system/carbon-platform/tree/main/services/web-app#readme",
  "license": "Apache-2.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carbon-design-system/carbon-platform.git"
  },
  "scripts": {
    "build": "npm run clean && next build",
    "build:storybook": "build-storybook -o .storybook-static",
    "bundle": "node esbuild.js",
    "clean": "rimraf .carbon public/.carbon .next .dev",
    "dev": "next dev",
    "dev:all": "concurrently 'open http://localhost:3000 && next dev' 'npm run storybook'",
    "dev:clean": "npm run clean && npm run dev",
    "dev:secure": "sh scripts/create-local-certificates.sh && concurrently 'npm run dev' 'RUNNING_SECURELY=1 npm run start:proxy'",
    "service:build": "npm run build",
    "start": "concurrently 'npm run start:proxy' 'next start'",
    "start:proxy": "node proxy-server.js",
    "storybook": "start-storybook -p 6006",
    "test": "echo 'No tests defined yet!'"
  },
  "bugs": {
    "url": "https://github.com/carbon-design-system/carbon-platform/issues"
  },
  "dependencies": {
    "@carbon/pictograms-react": "^11.25.0",
    "@carbon/react": "~0.16.2",
    "@mdx-js/loader": "^2.1.0",
    "@next/mdx": "^12.1.2",
    "@octokit/core": "^3.6.0",
    "ajv": "^8.11.0",
    "cache-manager": "^3.6.0",
    "cache-manager-fs-hash": "^1.0.0",
    "clsx": "^1.1.1",
    "concurrently": "^7.1.0",
    "cookie-parser": "^1.4.6",
    "css-loader": "^6.7.1",
    "express": "^4.17.3",
    "express-session": "^1.17.2",
    "file-loader": "^6.2.0",
    "fs-extra": "^10.0.1",
    "gray-matter": "^4.0.3",
    "http-proxy-middleware": "^2.0.4",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "minimatch": "^5.0.1",
    "next": "^12.1.2",
    "next-connect": "^0.12.2",
    "next-seo": "^5.3.0",
    "prop-types": "^15.8.1",
    "query-string": "^7.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remark-gfm": "^3.0.1",
    "remark-unwrap-images": "^3.0.1",
    "rimraf": "^3.0.2",
    "sass": "^1.49.9",
    "slugify": "^1.6.5",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.4.22",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-links": "^6.4.22",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack5": "^6.4.22",
    "@storybook/manager-webpack5": "^6.4.22",
    "@storybook/react": "^6.4.22",
    "sass-loader": "^12.6.0",
    "storybook-addon-next-router": "^3.1.1"
  }
}

{
  "name": "@carbon-platform/web-app",
  "version": "0.1.67",
  "description": "Carbon web application",
  "homepage": "https://github.com/carbon-design-system/carbon-platform/tree/main/services/web-app#readme",
  "license": "Apache-2.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carbon-design-system/carbon-platform.git"
  },
  "scripts": {
    "build": "npm run clean && next build",
    "clean": "rimraf .carbon public/.carbon .next .dev",
    "clean:data": "rimraf public/data",
    "dev": "next dev",
    "dev:clean": "npm run clean && next dev",
    "dev:secure": "sh ./scripts/create-local-certificates.sh && RUNNING_SECURELY=1 NODE_TLS_REJECT_UNAUTHORIZED=0 node secure-dev-server.js",
    "dev:secure:clean": "npm run clean && npm run dev:secure",
    "lint": "next lint",
    "service:build": "npm run build",
    "start": "node server.js",
    "test": "echo 'No tests defined yet!'"
  },
  "bugs": {
    "url": "https://github.com/carbon-design-system/carbon-platform/issues"
  },
  "dependencies": {
    "@carbon/pictograms-react": "^11.22.0",

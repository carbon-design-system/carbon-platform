  "license": "Apache-2.0",
  "author": "IBM",
  "private": true,
  "homepage": "https://github.com/carbon-design-system/carbon-platform/tree/main/services/web-app#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carbon-design-system/carbon-platform.git",
    "directory": "services/web-app"
  },
  "bugs": {
    "url": "https://github.com/carbon-design-system/carbon-platform/issues?q=is%3Aopen+is%3Aissue+label%3A%22service%3A+web-app+%F0%9F%8C%8E%22"
  },
  "scripts": {
    "build": "npm run clean && next build",
    "build:storybook": "build-storybook -o .storybook-static",
    "bundle": "node esbuild.mjs",
    "clean": "rimraf .carbon public/.carbon .next .dev out dist",
    "dev": "next dev",
    "dev:all": "concurrently 'open http://localhost:3000 && next dev -p 3000' 'npm run storybook'",
    "dev:clean": "npm run clean && npm run dev",
    "dev:secure": "sh scripts/create-local-certificates.sh && concurrently 'npm run dev' 'RUNNING_SECURELY=1 npm run start:proxy'",
    "service:build": "npm run build",
    "start": "concurrently --kill-others 'npm run start:proxy' 'next start -p 3000'",
    "start:proxy": "node proxy-server.mjs",
    "storybook": "start-storybook -p 6006",
    "test": "echo 'No tests defined yet!'"
  },

{
  "name": "@carbon-platform/web-app",
  "version": "0.1.4",
  "description": "Carbon web application",
  "homepage": "https://github.com/carbon-design-system/carbon-platform/tree/main/services/web-app#readme",
  "license": "Apache-2.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carbon-design-system/carbon-platform.git"
  },
  "scripts": {
    "build": "npm run clean && npm run build:fix-v11 && next build",
    "build:fix-v11": "sh ./fix-v11-carbon-prefix.sh",
    "build:svg": "svgr --replace-attr-values \"#000=currentColor\" -d icons svg",
    "clean": "rimraf .carbon .next",
    "dev": "next dev",
    "lint": "next lint",
    "service:build": "npm run build",
    "start": "next start",
    "test": "echo \"Error: run tests from root\" && exit 1"
  },
  "bugs": {
    "url": "https://github.com/carbon-design-system/carbon-platform/issues"
  },
  "dependencies": {
    "@carbon/react": "^0.7.0",
    "@octokit/core": "^3.5.1",
    "cache-manager": "^3.4.4",
    "cache-manager-fs-hash": "^1.0.0",
    "clsx": "^1.1.1",
    "js-yaml": "^4.1.0",
    "next": "^11.1.2",
    "next-seo": "^4.28.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "slugify": "^1.6.1"
  },
  "devDependencies": {
    "@svgr/cli": "^5.5.0",
    "css-loader": "^6.4.0",
    "sass": "^1.43.3"
  }
